{
  "name": "snapp-frontend-assignment",
  "version": "1.0.0",
  "description": "React project built from scratch with Webpack and TypeScript",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=\"--import tsx --no-deprecation\" webpack serve --mode development --port 3000 --env analyze=true analyzerPort=3001",
    "build": "cross-env NODE_OPTIONS=\"--import tsx --no-deprecation\" webpack --mode development",
    "build:prod": "cross-env NODE_OPTIONS=\"--import tsx --no-deprecation\" webpack --mode production",
    "build:analyze": "cross-env NODE_OPTIONS=\"--import tsx --no-deprecation\" webpack --mode production --env analyze=true",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,scss,css}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx,scss,css} --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,scss,css}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,scss,css}",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "husky-init": "husky"
  },
  "keywords": [
    "react",
    "webpack",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
